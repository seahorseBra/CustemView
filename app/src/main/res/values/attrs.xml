<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AlmanacItemView">
        <attr name="rank_orientation" format="string"/>
        <attr name="content_orientation" format="string"/>
        <attr name="contentTextColor" format="color"/>
        <attr name="imageRes" format="reference"/>
        <attr name="maxLine" format="integer"/>
        <attr name="interval" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="ShadowImageView">
        <attr name="shadowDx" format="float"/>
        <attr name="shadowDy" format="float"/>
        <attr name="shadowColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="ShadowImage">
        <attr name="shadowDX" format="integer" />
        <attr name="shadowDY" format="integer" />
        <attr name="shadowTint" format="color" />
        <attr name="res" format="integer"/>
    </declare-styleable>
</resources>